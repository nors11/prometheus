    "jimp": "^0.22.10",
    "mongoose": "^7.6.1",
    "mqtt": "^5.1.2",
    "nestjs-i18n": "^10.3.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.23.0",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/babel__generator": "^7.6.5",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/multer": "^1.4.8",
